        "template": "sap.ovp.cards.charts.analytical",
        "settings": {
          "title": "{{salaryTrendByTimeTitle}}",
          "subTitle": "{{salaryTrendByTimeSubTitle}}",
          "entitySet": "RequisitionReport",
          "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#SalaryTrendByTime",
          "presentationAnnotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant#SalaryTrendByTime"
        }
      }
    },
    "chartSettings": {
      "chartType": "analytical",
      "enableAutoBinding": true
    }
  },
  "sap.fiori": {
    "registrationIds": [],
