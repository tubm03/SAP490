      "TrendByStatusCard": {
        "model": "mainModel",
        "template": "sap.ovp.cards.charts.analytical",
        "settings": {
          "title": "{{trendByStatusTitle}}",
          "subTitle": "{{trendByStatusSubTitle}}",
          "entitySet": "RequisitionReport",
          "chartAnnotationPath": "com.sap.vocabularies.UI.v1.Chart#TrendByStatus",
